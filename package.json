    "hapi": ">=10.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "hapi": "13.x.x",
    "mocha": "2.5.3"
  }
